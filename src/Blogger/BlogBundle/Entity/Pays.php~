<?php
namespace Blogger\BlogBundle\Entity;

use Doctrine\ORM\Mapping as ORM;

/**
 * @ORM\Entity
 * @ORM\Table(name="pays")
 */
class Pays {

    /**
     * @ORM\Id
     * @ORM\Column(type="integer")
     * @ORM\GeneratedValue(strategy="AUTO")
     */
    protected $id;

    /**
     * @ORM\Column(type="string")
     */
    protected $date;

    /**
     * @ORM\Column(type="integer")
     */
    protected $m;

    /**
     * @ORM\Column(type="float")
     */
    protected $pay;

    /**
     * @ORM\Column(type="float")
     */
    protected $perc;

    /**
     * @ORM\Column(type="float")
     */
    protected $osn;

}
